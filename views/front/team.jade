extends layout

block content

    // page loading spinner
    .pageload
      .loader
    // /page loading spinner
    // menu navigation
    header.header.fixed-sticky
      .container
        nav.heading-font
          // branding
          .branding
            // toggle menu
            button.mobile-toggle(type='button')
              span.ti-menu
            // /toggle menu
            a.logo.transition(href='index.html')
              | Sublime
          // /branding
          // links
          .navigation.spy
            ul.nav
              li
                a.transition(href='index.html#how') How it works
              li
                a.transition(href='index.html#features') Features
              li
                a.transition(href='index.html#reviews') Reviews
              li
                a.transition(href='pricing.html') Pricing
              li.dropdown.show-on-hover.active
                a.ignore(href='javascript:;', data-toggle='dropdown')
                  span Pages
                ul.dropdown-menu
                  li.active
                    a.transition(href='contact.html') Support (Working Contact Form)
                  li
                    a.transition(href='gallery.html') Gallery
                  li
                    a.transition(href='team.html') Meet The Team
                  li
                    a.transition(href='elements.html') Elements
                  li
                    a.transition(href='blog.html') Blog
                  li
                    a.transition(href='post.html') Single blog item
                  li
                    a.transition(href='feature.html') Feature View
                  li
                    a.transition(href='404.html') Frontend 404 page
              li
                a(href='javascript:;') Login
              li
                a.nav-btn.btn.btn-success.btn-rounded(href='javascript:;') Sign Up
          // /links
    // /menu navigation
    // hero
    section#top.content-section.hero.vertical-center.color-inherit
      .hero-parallax.parallax(style='background-image: url(img/cover3.jpg);')
      .overlay.overlay-colored-9
      .container.no-p
        .hero-container
          h4.heading-font Meet Our Team
          .hero-sub-title
            p
              em Creative responsible for building our app
    // /hero
